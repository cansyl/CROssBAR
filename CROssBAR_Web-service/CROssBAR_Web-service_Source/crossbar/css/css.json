[
{
  "selector": "node",
  "style": {
    "background-color": "#666",
    "text-opacity"	  : "0.5",
    "label"		  	  : "data(display_name)",
	"font-size"    	  : "10px",
	"text-valign"  	  : "center",
    "text-halign"  	  : "center",
	"text-wrap"	 	  : "ellipsis",
	"text-max-width"  : 175
  }
},
{
  "selector": "node.highlightednode",
  "style": {
    "text-opacity"	  : "1",
    "font-size"		  : "15px",
	"text-wrap"	 	  : "wrap",
	"text-max-width"  : 200
  }
},
{
  "selector": "node:selected",
  "style": {
    "text-opacity": "1",
    "font-size": "15px"
  }
},
{
	"selector": "node.highlight",
	"style": {
		"border-color"	  : "#000000",
		"border-width"	  : "2px",
		"font-size"		  : "15px",
		"text-wrap"	 	  : "wrap",
		"text-max-width"  : 250
	}
},
{
	"selector": "node.semitransp",
	"style":{ "opacity": "0.5" }
},
{
	"selector": "edge.highlight",
	"style": { "width":"3px","opacity":"1","font-size": "10px",
	"mid-target-arrow-color": "#FFF" }
},
{
	"selector": "edge.highlightededge",
	"style": { "width":"3px","opacity":"1","font-size": "10px"}
},
{
	"selector": "edge.semitransp",
	"style":{ "opacity": "0.2" }
},
{
  "selector": "node[Node_Type='Pathway']",
  "style": {
    "shape": "diamond",
    "background-color": "#00b0f0"
  }
},
{
  "selector": "node[Node_Type='kegg_Pathway']",
  "style": {
    "shape": "diamond",
    "background-color": "#07779F"
  }
},
{
  "selector": "node[Node_Type='Disease']",
  "style": {
    "shape": "triangle",
    "background-color": "#c00000"
  }
},
{
  "selector": "node[Node_Type='kegg_Disease']",
  "style": {
    "shape": "triangle",
    "background-color": "#EC7063"
  }
},
{
  "selector": "node[Node_Type='Protein']",
  "style": {
    "shape": "ellipse",
    "background-color": "darkorange"
  }
},
{
  "selector": "node[Node_Type='Protein_N']",
  "style": {
    "shape": "ellipse",
    "background-color": "#F8C471"
  }
},
{
  "selector": "node[Node_Type='HPO']",
  "style": {
    "shape": "pentagon",
    "background-color": "#7030a0"
  }
},
{
  "selector": "node[Node_Type='Drug']",
  "style": {
    "shape": "hexagon",
    "background-color": "white",
	"border-width": 2,
	"border-color": "green"
  }
},
{
  "selector": "node[Node_Type='Compound']",
  "style": {
    "shape": "hexagon",
    "background-color": "white",
	"label": "data(id)",
	"border-width": 2,
	"border-color": "violet"
  }
},
{
  "selector": "node[Node_Type='Prediction']",
  "style": {
    "shape": "hexagon",
    "background-color": "white",
	"label": "data(id)",
	"border-width": 2,
	"border-color": "violet"	
  }
},
{
  "selector": "edge",
  "style": {
	"width": "1px",
	"line-color": "#7f8c8d",
	"curve-style": "unbundled-bezier",
	"opacity": "0.25",
	"label": "data(label)",
	"text-rotation": "autorotate",
	"text-margin-x": "10px",
	"font-size": "0px"
  }
},
{
  "selector": "edge[Edge_Type='Drug']",
  "style": {
	"line-color": "#078307"
  }
},
{
  "selector": "edge[Edge_Type='Chembl']",
  "style": {
	"line-color": "#5a90d4"
  }
},
{
  "selector": "edge[Edge_Type='predictionChembl']",
  "style": {
	"line-color": "#5a90d4"
  }
},
{
  "selector": "edge[Edge_Type='drugChembl']",
  "style": {
	"line-color": "#5a90d4"
  }
},
{
  "selector": "edge[Edge_Type='drugPrediction']",
  "style": {
	"line-color": "#d03f3f"
  }
},
{
  "selector": "edge[Edge_Type='compoundPrediction']",
  "style": {
	"line-color": "#d03f3f"
  }
},
{
  "selector": "edge[Edge_Type='Prediction']",
  "style": {
	"line-color": "#d03f3f"
  }
},
{
  "selector": "edge[Edge_Type='PredChem']",
  "style": {
	"line-color": "violet"
  }
}
]